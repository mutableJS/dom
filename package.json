{
	"name": "@mutablejs/dom",
	"version": "0.3.2",
	"description": "Reactive DOM elements, based on mutableJS's state",
	"keywords": [
		"mutable DOM",
		"reactive DOM",
		"dynamic DOM",
		"DOM manipulation",
		"mutableJS",
		"JSX",
		"reactive JSX",
		"web app"
	],
	"files": [
		"dist"
	],
	"main": "./dist/index.umd.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"require": "./dist/index.umd.js"
		}
	},
	"scripts": {
		"start": "npx vite",
		"build": "npx vite build && npx tsc --emitDeclarationOnly",
		"prepack": "npm run build"
	},
	"author": "Nikita 'donnikitos' Nitichevski <me@donnikitos.com> (https://donnikitos.com/)",
	"repository": {
		"type": "git",
		"url": "https://github.com/mutableJS/dom"
	},
	"license": "MIT",
	"engines": {
		"node": "^14.18.0 || >=16.0.0"
	},
	"devDependencies": {
		"@mutablejs/core": "^0.2.1",
		"@types/node": "^20.8.6",
		"typescript": "^5.2.2",
		"vite": "^4.4.11"
	},
	"peerDependencies": {
		"@mutablejs/core": "^0.2.1"
	},
	"dependencies": {}
}
